interactive program {
  K_DOWN     -> { ProcessMoveTo_(just(South)) }
  K_UP       -> { ProcessMoveTo_(just(North)) }
  K_LEFT     -> { ProcessMoveTo_(just(East))  }    
  K_RIGHT    -> { ProcessMoveTo_(just(West))  }
  TIMEOUT(1) -> { ProcessMoveTo_(Nothing)     }
}

procedure ProcessMoveTo_(mdir) {
      WriteQuell_(level())
}