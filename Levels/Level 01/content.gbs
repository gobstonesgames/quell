/* */
interactive program {
  INIT       -> { MarkAsTutorial() 
                  WriteQuell_(level())        
                }
  K_R        -> { MarkAsTutorial() 
                  WriteQuell_(level())        
                }
  K_DOWN     -> { ProcessMoveTo_(South) }
  K_UP       -> { ProcessMoveTo_(North) }
  K_LEFT     -> { ProcessMoveTo_(West)  }    
  K_RIGHT    -> { ProcessMoveTo_(East)  }
  TIMEOUT(1) -> { ProcessMove()         }
}
