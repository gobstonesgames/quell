function level() {
  return(QuellLevel(
           isTutorial <- False
         , board      <- board01()
         , solution   <- [East,North,West, South,East]
         , minMoves   <- 5
         , jewel      <- QJewel(jpos <- pos(2,2), hits <- 4)
         , spikesDir  <- Nothing
         , boxes      <- []
         , ball       <- QBallAt(bpos <- pos(5,3))
         , numMoves   <- 0
         , rightMoves <- just(0)
         , numPearls  <- 4
  ))
}

