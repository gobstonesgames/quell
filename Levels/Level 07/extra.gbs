function level() {
  return(QuellLevel(
           status     <- RegularLevel
         , board      <- board07()
         , solution   <- [ East, South, East ]
         , minMoves   <- 3
         , jewel      <- QJewel(jpos <- pos(5,5), hits <- 4)
         , spikesDir  <- Nothing
         , boxes      <- []
         , ball       <- QBallAt(bpos <- pos(10,4))
         , numMoves   <- 0
         , rightMoves <- just(0)
         , numPearls  <- 4
  ))
}

