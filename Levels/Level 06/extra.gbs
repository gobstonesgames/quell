function level() {
  return(QuellLevel(
           status     <- RegularLevel
         , board      <- board06()
         , solution   <- [ East, South, West, North, West, South ]
         , minMoves   <- 6
         , jewel      <- QJewel(jpos <- pos(12,5), hits <- 4)
         , spikesDir  <- Nothing
         , boxes      <- []
         , ball       <- QBallAt(bpos <- pos(4,4))
         , numMoves   <- 0
         , rightMoves <- just(0)
         , numPearls  <- 3
  ))
}

