function level() {
  return(QuellLevel(
           status     <- TutorialLevel
         , board      <- boardT02()
         , solution   <- [ East, South, West, North, East ]
         , minMoves   <- 5
         , jewel      <- NoJewel
         , spikesDir  <- Nothing
         , boxes      <- []
         , ball       <- QBallAt(bpos <- pos(3,6))
         , numMoves   <- 0
         , rightMoves <- just(0)
         , numPearls  <- 1
  ))
}

