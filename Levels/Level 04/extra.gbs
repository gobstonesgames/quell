function level()   {
  return(QuellLevel(
           status     <- RegularLevel
         , board      <- board04()  
         , solution   <-  [ East, North, West, East, South, West, North, East, North, West, South ]
         , minMoves   <- 11
         , jewel      <- QJewel(jpos <- pos(1,3), hits <- 4)
         , spikesDir  <- Nothing
         , boxes      <- []
         , ball       <- QBallAt(bpos <- pos(3,2))
         , numMoves   <- 0
         , rightMoves <- just(0)
         , numPearls  <- 3
  ))
}

