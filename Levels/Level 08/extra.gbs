function level() {
  return(QuellLevel(
           status     <- RegularLevel
         , board      <- board08()
         , solution   <- [ West, South, East, North, West, North, East, South, East, North ]
         , minMoves   <- 10
         , jewel      <- QJewel(jpos <- pos(5,1), hits <- 4)
         , spikesDir  <- Nothing
         , boxes      <- []
         , ball       <- QBallAt(bpos <- pos(6,5))
         , numMoves   <- 0
         , rightMoves <- just(0)
         , numPearls  <- 2
  ))
}

function board08() { 
  // [[QElem]], size=pos(14,9)
  return(
   [[ cNW(), hT() , hT() , hT() , hT() , hT() , hT() , hT() , hT() , hT() , hT() , hT() , hT() , cNE() ]
   ,[ vL() , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , vR()  ]
   ,[ vL() , Wall , sS() , Empty, Empty, Empty, Empty, Empty, Empty, Wall , Pearl, Wall , Wall , vR()  ]
   ,[ vL() , Wall , Empty, Empty, oG() , Empty, Empty, Empty, Empty, Empty, Empty, Empty, Wall , vR()  ]
   ,[ vL() , Wall , Empty, Empty, Empty, Empty, Empty, Empty, Empty, Empty, Empty, Empty, Wall , vR()  ]
   ,[ vL() , Wall , Pearl, Empty, Empty, Empty, Empty, Empty, Empty, Empty, Empty, Empty, Wall , vR()  ]
   ,[ vL() , Wall , Wall , Empty, Empty, Empty, mS() , Empty, Empty, Empty, Empty, Wall , Wall , vR()  ]
   ,[ vL() , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , Wall , vR()  ]
   ,[ cSW(), hB() , hB() , hB() , hB() , hB() , hB() , hB() , hB() , hB() , hB() , hB() , hB() , cSE() ]
   ])
}